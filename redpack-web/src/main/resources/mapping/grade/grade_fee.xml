		</if>		
		<if test="beforeUpgrade != null">  
				,before_upgrade = #{beforeUpgrade}
		</if>		
		<if test="afterUpgrade != null">  
				,after_upgrade = #{afterUpgrade}
		</if>		

		WHERE id = #{id} and version =  #{version}
